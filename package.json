{
  "name": "agnostic-web-foundation",
  "version": "0.0.1",
  "build": 0,
  "author": {
    "name": "Eduardo Almeida",
    "url": "https://github.com/web2solutions",
    "email": "web2solucoes@gmail.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/web2solutions/agnostic-web-foundation.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "webpack": "webpack",
    "_test": "npm run lint && mocha --recursive --exit --require @babel/register",
    "build:tester:foundation": "npx webpack --config webpack.config.js --entry ./test/Foundation.js --output-path ./tests-bundle/Foundation",
    "build:tester:localtransport": "npx webpack --config webpack.config.js --entry ./test/LocalDatabaseTransport.js --output-path ./tests-bundle/LocalDatabaseTransport",
    "build:tester:utils": "npx webpack --config webpack.config.js --entry ./test/utils.js --output-path ./tests-bundle/utils",
    "build:testers": "npm run build:tester:foundation && npm run build:tester:localtransport && npm run build:tester:utils",
    "test": "npm run build:testers && karma start --single-run --browsers ChromeHeadless,FirefoxHeadless karma.config.js",
    "lint": "eslint src --ext .js",
    "eslint-fix": "eslint src/**/**/**/**/**/*.js --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\"",
    "doc": "npx jsdoc -c ./jsDoc.json -t ./node_modules/ink-docstrap/template -R ./docs/README.md",
    "build": "npm run rm && npm run lint && npm run doc && npm run webpack",
    "rm": "rm -rf ./dist && mkdir dist/ && cp -a html_app/. dist",
    "start:dev": "webpack serve"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "^4.5.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "bootstrap": "^4.6.0",
    "core-js": "^2.6.12",
    "dexie": "^3.0.3",
    "dexie-mongoify": "^1.3.0",
    "lunr": "^2.3.9",
    "moment": "^2.29.1",
    "mongoose": "^5.11.12",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "recharts": "^2.0.4",
    "sweetalert": "^2.1.2"
  },
  "description": ""
}
